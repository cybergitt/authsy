{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "dynamicImport": true,
        "decorators": true
      },
      "transform": null,
      "target": "esnext",
      "baseUrl": ".",
      "loose": true,
      "externalHelpers": false,
      "keepClassNames": true
    },
    "module": {
      "type": "es6",
      "strict": false,
      "strictMode": false,
      "noInterop": false,
      "lazy": false
    },
    "minify": true,
    "sourceMaps": true
  }