{
  "name": "express-auth",
  "version": "1.0.0",
  "description": "A simple user authentication APIs to authenticate users",
  "main": "index.js",
  "engines": {
    "node": ">=18.17.0",
    "pnpm": ">=9.3.0",
    "npm": "please-use-pnpm"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "dev": "ts-node-dev --respawn --clear --transpile-only --ignore-watch node_modules ./src/app.ts",
    "type-check": "tsc",
    "prebuild": "rimraf ./dist",
    "build": "pnpm test && pnpm prebuild && tsc",
    "start": "node dist/app.js"
  },
  "keywords": [
    "API",
    "REST",
    "Express"
  ],
  "author": "Sigit Prayitno",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "rimraf": "^5.0.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  }
}
